  "dependencies": {
    "@rebass/grid": "^6.0.0-4",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.4",
    "react": "^16.6.1",
    "react-dom": "^16.6.1",
    "react-scripts": "2.1.1",
    "react-select": "^2.1.1",
    "styled-components": "^4.0.3",
